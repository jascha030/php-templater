{
    "name": "jascha030/php-templater",
    "description": "A templater console app used to boilerplate files",
    "require": {
        "symfony/console": "^5.2",
        "symfony/filesystem": "^5.2",
        "twig/twig": "^3.3",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jascha030",
            "email": "contact@jaschavanaalst.nl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Jascha030\\PTemplater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jascha030\\PTemplater\\": "src"
        }
    }
}
